<odoo>
    <record id="sh_type_view_tree" model="ir.ui.view">
        <field name="name">sh.type.view.tree</field>
        <field name="model">sh.type</field>
        <field name="arch" type="xml">
            <tree string="type">
                <field name="product_type" />
                <field name="dom" />
                <field name="doe" />

            </tree>
        </field>
    </record>
    <record id="sh_type_view_form" model="ir.ui.view">
        <field name="name">sh.type.view.form</field>
        <field name="model">sh.type</field>
        <field name="arch" type="xml">
            <form string="type">
                <sheet>
                    <group>
                        <field name="product_type" />
                        <field name="dom" />
                        <field name="doe" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>